{
  "name": "nuxt-typescript",
  "version": "1.0.0",
  "description": "Nuxt.js Prototype Project",
  "author": "Hirokazu Miyaji",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "precommit": "yarn lint",
